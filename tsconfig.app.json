{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*", "src/**/*.vue",
    "docs/**/*", "docs/**/*.vue",
    "preview/**/*", "preview/**/*.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "tulp*": ["./src/*"],
      "docs": ["./docs/*"],
      "preview": ["./preview/*"]
    }
  }
}
